{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://*:5003",
        "Certificate": {
          "KeyPath": "/app/certs/tls.key",
          "Path": "/app/certs/tls.crt"
        }
      }
    },
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  }
}
